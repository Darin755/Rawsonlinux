#!/bin/bash
apt install xfce4 xfce4-goodies lightdm varity cmatrix sfdashboard clamtk clamav libreoffice firefox wmctrl docky
sudo apt install xfce4 xfce4-goodies lightdm variety cmatrix xfdashboard clamtk clamav libreoffice firefox wmctrl unzip
cp config.zip $HOME
cd
unzip config.zip
cd RawsonlinuxScripts 
cp * /bin/ 
cd
rm config.zip
rm -r -f RawsonlinuxScripts
exit 0
